{
  "hosting": {
    "public": "label-verifier-ui/dist/label-verifier-ui/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/**",
        "run": {
          "serviceId": "alv-backend-gpu",
          "region": "us-central1"
        }
      }
    ]
  }
}
